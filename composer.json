        "chaoticwave/blue-velvet": "1.0.*",
        "lusitanian/oauth":        "~0.3"
    },
    "require-dev":       {
        "barryvdh/laravel-ide-helper": "^2.2",
        "fzaninotto/faker":            "~1.4",
        "phpunit/phpunit":             "~4.0",
        "laravel/framework":           "5.2.*"
    },
    "autoload":          {
        "psr-4": {
            "ChaoticWave\\Twister\\": "src/"
        }
    },
    "autoload-dev":      {
        "classmap": []
    },
    "extra":             {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        }
    }
