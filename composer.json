{
    "name":              "chaoticwave/twister",
    "description":       "Laravel-Friendly Twitter Service",
    "license":           "MIT",
    "type":              "library",
    "authors":           [
        {
            "name":  "Jerry Ablan",
            "email": "jerryablan@gmail.com"
        }
    ],
    "keywords":          [
        "utility",
        "laravel",
        "tweet",
        "twitter"
    ],
    "minimum-stability": "stable",
    "require":           {
